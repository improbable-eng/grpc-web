{
  "name": "grpc-web-integration-test",
  "version": "0.14.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ts/build && rm -rf ts/build-node",
    "build:testserver": "cd go && go build -o ./build/testserver ./testserver/testserver.go",
    "build:proto": "cd proto && buf generate",
    "build:ts": "cd ts && rm -rf build && webpack-cli",
    "build:dev": "cd ts && rm -rf build && webpack-cli --watch",
    "build:node": "cd ts && rm -rf build-node && (cd node-src && tsc) && (cp -R _proto build-node/integration_test/ts)",
    "build": "npm run build:proto && npm run build:ts && npm run build:node && npm run build:testserver",
    "start:ci": "./test-ci.sh",
    "start": "./test.sh",
    "test:browsers": "./run-with-tunnel.sh ./run-with-testserver.sh karma start ./karma.conf.ts --single-run",
    "test:node": "BROWSER=nodejs ./run-with-testserver.sh jasmine ts/build-node/integration_test/ts/node-src/node.spec.js",
    "test:dev": "npm run build && ./run-with-testserver.sh karma start ./karma.conf.ts"
  },
  "license": "none",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.14.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.14.0",
    "browser-headers": "^0.4.1",
    "event-stream": "^4.0.1",
    "google-protobuf": "3.14.0",
    "typedarray": "0.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/chai": "^4.2.14",
    "@types/google-protobuf": "^3.7.4",
    "@types/jasmine": "^3.6.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "^16.3.2",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "jasmine": "3.6.4",
    "jasmine-core": "3.6.0",
    "karma": "6.2.0",
    "karma-jasmine": "4.0.1",
    "karma-junit-reporter": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "lodash": "^4.17.20",
    "sauce-connect-launcher": "^1.3.2",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "ts-protoc-gen": "0.14.0",
    "typescript": "4.1.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  }
}
