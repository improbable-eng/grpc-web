{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "target": "es5",
    "removeComments": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "../build-node",
    "noEmitOnError": true
  },
  "types": [
    "jasmine",
    "node"
  ],
  "include": [
    "./node.spec.ts"
  ]
}
