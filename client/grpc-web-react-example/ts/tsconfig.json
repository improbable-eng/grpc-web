{
  "compilerOptions": {
    "alwaysStrict": true,
    "sourceMap": true,
    "target": "es5",
    "removeComments": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true
  }
}
